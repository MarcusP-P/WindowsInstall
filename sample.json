				},
				{
					"Type": "winget",
					"Id": "Notepad++.Notepad++"
				},
				{
					"Type": "winget",
					"Id": "PuTTY.PuTTY"
				},
				{
					"Type": "winget",
					"Id": "TortoiseGit.TortoiseGit"
				},
				{
					"Type": "winget",
					"Id": "vim.vim"
				},
				{
					"Type": "winget",
